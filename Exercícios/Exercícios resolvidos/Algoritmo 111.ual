#Algoritmo 111
#Entrar com dois números e imprimir o maior número (suponha números diferentes).

prog Algoritmo111

     int num1, num2;
     imprima "\nInforme um numero inteiro: ";
     leia num1;
     imprima "\nInforme outro numero inteiro: ";
     leia num2;
     se (num1 > num2)
     {
     imprima "\nO numero ", num1, " e maior";
     }
     se (num1 < num2)
     {
     imprima "\nO numero ", num2, " e maior";
     }
     imprima "\n\n";
     
fimprog