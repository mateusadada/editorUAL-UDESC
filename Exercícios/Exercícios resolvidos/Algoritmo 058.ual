prog Algoritmo58

     real x1, x2, x3;
     
     imprima "\nDigite o valor de x1: ";
     leia x1;
     
     imprima "\nDigite o valor de x2: ";
     leia x2;
     
     imprima "\nDigite o valor de x3: ";
     leia x3;
          
     imprima "\nX = ", formatar((x1 + (x2 / (x3+x1)) + 2*(x1 - x2) + (log(64) * log(2))),2);

     imprima "\n\n";
fimprog