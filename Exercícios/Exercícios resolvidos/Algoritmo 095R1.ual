#Algoritmo 95 
#Entrar com um número e informar se ele é ou não divisível por 5. 

prog Algoritmo95R1

     int num;
     imprima "\nInforme um numero: ";
     leia num;
     se (num % 5 == 0)
     {
        imprima "\nO numero eh divisivel por 5";
     }
     senao
     {
        imprima "\nO numero NAO eh divisivel por 5";
     }
     imprima "\n\n";
     
fimprog     