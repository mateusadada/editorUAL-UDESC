prog vetor5

   int i, media;
   real valor[5], soma;
     
   soma <- 0.0;
   media <- 0;
     
   para (i <- 0; i < 5; i++)
   {
      imprima "Digite um numero real: ";
      leia valor[i];
      soma <- soma + valor[i];
      media++;
   }
     
   imprima "\nMedia dos numeros: ", formatar(soma / media,2);

   imprima "\n\n";
fimprog