#Algoritmo 112
#Entrar com dois números e imprimir o menor numero (suponha números diferentes).

prog Algoritmo112

     int num1, num2;
     imprima "\nInforme um numero inteiro: ";
     leia num1;
     imprima "\nInforme outro numero inteiro: ";
     leia num2;
     se (num1 < num2)
     {
     imprima "\nO numero ", num1, " e menor";
     }
     se (num1 > num2)
     {
     imprima "\nO numero ", num2, " e menor";
     }
     imprima "\n\n";
     
fimprog