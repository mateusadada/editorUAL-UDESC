prog Algoritmo63

     real ValorHora, INSS, salarioFinal;
     int NumeroAulas;
     
     imprima "\nDigite o valor da hora aula: R$ ";
     leia ValorHora;
     
     imprima "Digite o numero de aulas dadas no mes: ";
     leia NumeroAulas;
     
     imprima "Digite o percentual de desconto do INSS: ";
     leia INSS;
     
     salarioFinal <- (ValorHora * NumeroAulas) * ((100.0 - INSS)/100);

     imprima "\nSalario liquido: R$ ", salarioFinal;

     imprima "\n\n";
fimprog
