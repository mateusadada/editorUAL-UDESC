prog Algoritmo265

     int numero, contador;
     
     numero <- 1;
     contador <- 0;
     
     imprima "\n";
     
     enquanto (numero <> 0)
     {
          imprima "Digite um numero: ";
          leia numero;
          
          se ((numero >= 100) && (numero <= 200))
          {
               contador++;
          }
     }
     
     imprima "\nQuantos numeros digitados estao entre 100 e 200: ", contador;

     imprima "\n\n";
fimprog