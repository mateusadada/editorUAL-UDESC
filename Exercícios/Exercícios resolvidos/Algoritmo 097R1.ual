#Algoritmo 97
#Entrar com um número e informar se ele é divisível por 10, por 5, por 2 ou se não é divisível por nenhum destes.

prog Algoritmo97R1

     int num;
     imprima "\nInforme um numero: ";
     leia num;
     se ((num % 10 == 0) && (num % 5 == 0) && (num % 2 == 0))
     {
        imprima "\nO valor eh divisivel por 10, 5 ou 2";
     }
     senao
     {
        imprima "\nO valor NAO eh divisivel por 10, 5 ou 2";
     }
     imprima "\n\n";
     
fimprog