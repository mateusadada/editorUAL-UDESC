prog vetor3

     int i, numeros[5];
     
     para (i <- 0; i < 5; i++)
     {   imprima "Digite um numero inteiro: ";
         leia numeros[i];
     }

     para (i <- 0; i < 5; i++)
     {   se ((numeros[i] % 2) == 1)
         {
            imprima "\nNumero impar: ", numeros[i];
         }
     }

     imprima "\n\n";
fimprog